import java.util.Scanner;
class ele
{
    public static void main(String args[])
{
    Scanner s=new Scanner(System.in);
    int N=s.nextInt();
    int i,sum=0,r,cnt=0;
    int a[]=new int[60];
    for(i=0;i<N;i++)
    {
        a[i]=s.nextInt();
    }
    for(i=0;i<N;i++)
    {
        if(a[i]<a[i+1])
        {
          cnt++;
        }
    }
    if(cnt==(N-1))
    System.out.println("yes");
    else
    System.out.println("no");
}
}
