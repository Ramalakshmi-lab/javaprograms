import java.util.Scanner;
class divisors
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int p=s.nextInt();
        int q=s.nextInt();
        int r=s.nextInt();
        int c=0;
        for(int i=p;i<=q;i++)
        {
            if(i%r==0)
            {
                c=c+1;
            }
        }
        System.out.println(c);
    }
}
