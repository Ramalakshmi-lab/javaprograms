import java.util.Scanner;
class div
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int n[]=new int[a];
        int k=0;
        int i;
        for(i=0;i<a;i++){
            n[i]=s.nextInt();
        }
        for(int j=0;j<a;j++){
            if(n[j]%b!=0)
            {
                k=k+1;
            }
        }
        System.out.println(k);
    }
}
        
