import java.util.Scanner;
class k_sum
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        int su=0;
        for(int i=0;i<n;i++)
        {
            a[i]=s.nextInt();
        }
        int k=s.nextInt();
        for(int i=0;i<k;i++)
        {
            su=su+a[i];
        }
        System.out.println(su);
    }
}
